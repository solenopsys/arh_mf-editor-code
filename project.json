{
  "name": "mf-code",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/modules/mf-code/src",
  "prefix": "ui",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/modules/mf-code",
        "index": "packages/modules/mf-code/src/index.html",
        "main": "packages/modules/mf-code/src/main.ts",
        "polyfills": "packages/modules/mf-code/src/polyfills.ts",
        "tsConfig": "packages/modules/mf-code/tsconfig.app.json",
        "assets": [
          "./packages/modules/mf-code/src/favicon.ico",
          "packages/modules/mf-code/src/assets"
        ],
        "styles": ["packages/modules/mf-code/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "packages/modules/mf-code/src/environments/environment.ts",
              "with": "packages/modules/mf-code/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "clickhouse:build:production"
        },
        "development": {
          "browserTarget": "clickhouse:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/uimatrix/ui-code/editors"
      ],
      "options": {
        "jestConfig": "packages/uimatrix/ui-editors/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/uimatrix/ui-editors/code/src/**/*.ts",
          "packages/uimatrix/ui-editors/code/src/**/*.html"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  },
  "tags": []
}
